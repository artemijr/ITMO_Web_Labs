<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>AreaCheckServlet</servlet-name>
        <servlet-class>main.AreaCheckServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AreaCheckServlet</servlet-name>
        <url-pattern>/AreaCheckServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ClearResultsServlet</servlet-name>
        <servlet-class>main.ClearResultsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClearResultsServlet</servlet-name>
        <url-pattern>/ClearResultsServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoadResultsServlet</servlet-name>
        <servlet-class>main.LoadResultsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadResultsServlet</servlet-name>
        <url-pattern>/LoadResultsServlet</url-pattern>
    </servlet-mapping>

</web-app>
